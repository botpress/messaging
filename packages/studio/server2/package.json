{
  "name": "@botpress/studio-server2",
  "version": "0.0.1",
  "main": "index.js",
  "license": "AGPL-3.0",
  "scripts": {
    "build:ncc": "yarn build && yarn run -T rimraf dist && yarn run -T ncc build src/index.ts -o dist -e electron",
    "build": "yarn && yarn run -T tsc --build",
    "build:tsc": "yarn run -T tsc --build",
    "watch": "yarn && yarn run -T tsc --build --watch",
    "start": "node ./dist/index.js",
    "dev": "yarn && yarn run -T ts-node-dev --debounce 500 --transpile-only ./src/index.ts"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.2",
    "@types/doctrine": "^0.0.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.33",
    "@types/uuid": "^8.3.4"
  },
  "dependencies": {
    "@botpress/framework": "1.2.0",
    "cli-color": "^2.0.2",
    "doctrine": "^3.0.0",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "knex": "^0.95.15",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "socket.io": "^4.5.0",
    "uuid": "^8.3.2",
    "yn": "4.0.0"
  }
}
