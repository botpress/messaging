{
  "name": "@botpress/messaging",
  "version": "0.1.21",
  "description": "Botpress messaging repo",
  "author": "Botpress, Inc.",
  "license": "AGPL-3.0",
  "scripts": {
    "eslint": "eslint packages/ --ext .ts",
    "prettier": "prettier --check './packages/**/*.ts' '!**/*.d.ts'",
    "clean": "rimraf packages/*/dist bin etc/jest/.test-data",
    "reset": "yarn clean && rimraf .parcel-cache packages/*/node_modules node_modules .yarn/cache .yarn/install-state.gz .yarn/unplugged .pnp.cjs .pnp.loader.mjs",
    "package": "yarn clean && yarn build && node ./etc/scripts/package",
    "release": "yarn node ./etc/scripts/release",
    "build": "yarn workspace @botpress/messaging-server build",
    "watch": "yarn workspace @botpress/messaging-server watch",
    "start": "yarn workspace @botpress/messaging-server start",
    "dev": "yarn workspace @botpress/messaging-server dev",
    "board": "yarn workspace @botpress/messaging-board dev",
    "channels": "yarn workspace @botpress/messaging-channels example",
    "test": "yarn test:unit && yarn test:int && yarn test:mig && yarn test:e2e && yarn test:sec",
    "test:unit": "jest -c ./etc/jest/jest.unit.config.ts",
    "test:int": "jest -c ./etc/jest/jest.integration.config.ts",
    "test:e2e": "jest -c ./etc/jest/jest.e2e.config.ts",
    "test:sec": "jest -c ./etc/jest/jest.security.config.ts",
    "test:mig": "jest -c ./etc/jest/jest.migration.config.ts"
  },
  "bin": "./packages/server/dist/index.js",
  "pkg": {
    "scripts": "./packages/server/dist/**/*.js",
    "assets": [
      "./node_modules/sqlite3/lib/binding/**/*"
    ],
    "targets": [
      "node16-win32-x64",
      "node16-linux-x64",
      "node16-macos-x64"
    ],
    "outputPath": "./bin"
  },
  "devDependencies": {
    "@jest/types": "^27.4.2",
    "@parcel/config-default": "^2.1.1",
    "@parcel/core": "^2.1.1",
    "@parcel/packager-ts": "^2.1.1",
    "@parcel/resolver-default": "^2.1.1",
    "@parcel/transformer-babel": "^2.1.1",
    "@parcel/transformer-css": "^2.1.1",
    "@parcel/transformer-html": "^2.1.1",
    "@parcel/transformer-js": "^2.1.1",
    "@parcel/transformer-postcss": "^2.1.1",
    "@parcel/transformer-posthtml": "^2.1.1",
    "@parcel/transformer-react-refresh-wrap": "^2.1.1",
    "@parcel/transformer-typescript-tsc": "^2.1.1",
    "@parcel/transformer-typescript-types": "^2.1.1",
    "@types/jest": "^27.4.0",
    "@types/jest-dev-server": "^5.0.0",
    "@types/node": "^16.11.13",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "bluebird": "^3.7.2",
    "bluebird-global": "^1.0.1",
    "chalk": "^4.1.2",
    "conventional-changelog": "^3.1.25",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "37.6.1",
    "fs-extra": "^10.0.0",
    "jest": "^27.4.7",
    "jest-dev-server": "^6.0.3",
    "jest-mock": "^27.4.6",
    "parcel": "^2.1.1",
    "pkg": "^5.5.1",
    "portfinder": "^1.0.28",
    "prettier": "^2.5.1",
    "pretty-format": "^27.4.6",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2",
    "yargs": "^17.3.1"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": "16.13.0"
  },
  "packageManager": "yarn@3.1.1",
  "alias": {
    "@botpress/messaging-base": "@botpress/messaging-base/src",
    "@botpress/messaging-client": "@botpress/messaging-client/src",
    "@botpress/messaging-socket": "@botpress/messaging-socket/src",
    "@botpress/messaging-components": "@botpress/messaging-components/src",
    "@botpress/webchat": "@botpress/webchat/src",
    "@botpress/chat": "@botpress/chat/src",
    "@botpress/chat-skin": "@botpress/chat-skin/src"
  },
  "dependenciesMeta": {
    "core-js@3.20.0": {
      "built": false
    },
    "core-js-pure@3.20.0": {
      "built": false
    }
  }
}
