{
  "name": "@botpress/messaging",
  "version": "0.0.2",
  "main": "index.ts",
  "license": "MIT",
  "bin": "./dist/index.js",
  "pkg": {
    "scripts": "./dist/**/*.js",
    "assets": [
      "./node_modules/sqlite3/lib/binding/**/*"
    ],
    "targets": [
      "node12-win32-x64",
      "node12-linux-x64",
      "node12-macos-x64"
    ],
    "outputPath": "./bin"
  },
  "scripts": {
    "build": "yarn --silent && tsc --build",
    "watch": "yarn --silent && tsc --build --watch",
    "start": "node dist/index.js",
    "dev": "yarn --silent && ts-node-dev --inspect --inspect-publish-uid=http --transpile-only src/index.ts",
    "clean": "rimraf dist bin",
    "reset": "yarn clean && rimraf node_modules",
    "eslint": "node_modules/.bin/eslint src --ext .ts",
    "prettier": "cross-env prettier --check './src/**/*.ts'",
    "package": "yarn --silent && yarn clean && yarn build && node ./scripts/package",
    "release": "node ./scripts/release"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cli-color": "^2.0.0",
    "@types/ioredis": "^4.26.4",
    "@types/lodash": "^4.14.168",
    "@types/lru-cache": "^5.1.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^15.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "conventional-changelog": "^3.1.24",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsdoc": "^34.8.1",
    "fs-extra": "^10.0.0",
    "pkg": "^4.3.7",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4",
    "yargs": "^17.0.1"
  },
  "dependencies": {
    "@slack/events-api": "^2.3.2",
    "@slack/interactive-messages": "^1.5.0",
    "@slack/rtm-api": "^5.0.3",
    "@slack/web-api": "^5.8.0",
    "@types/express": "^4.17.11",
    "@vonage/server-sdk": "2.10.7-beta-2",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "bluebird-global": "^1.0.1",
    "botbuilder": "^4.5.2",
    "cli-color": "^2.0.0",
    "discord-buttons": "^2.4.1",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "ioredis": "^4.27.3",
    "knex": "^0.95.4",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "moment": "^2.29.1",
    "ms": "^2.1.3",
    "pg": "^8.6.0",
    "portfinder": "^1.0.28",
    "smooch-core": "^8.11.4",
    "sqlite3": "^5.0.2",
    "telegraf": "^3.27.1",
    "twilio": "^3.61.0",
    "uuid": "^8.3.2"
  }
}
